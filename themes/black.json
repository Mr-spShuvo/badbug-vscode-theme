    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D16969"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression.begin",
